# Initialize a new Git repository
git init

# Create a README.md file and add a line to it
echo "# linux_commonds_note" >> README.md

# Stage the README.md file
git add README.md

# Commit the changes with a message
git commit -m "first commit"

# Rename the current branch to 'master'
git branch -M master

# Add a remote repository
git remote add origin https://github.com/suanx/linux_commonds_note.git

# Attempt to push changes to the 'main' branch (this failed because 'main' does not exist)
git push -u origin main

# Successfully push changes to the 'master' branch
git push -u origin master

